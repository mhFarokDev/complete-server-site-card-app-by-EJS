
    
    <div class="container">
        <div class="row justify-content-center my-5">
            <div class="col-md-8">
                <div class="card">
                    
                    <div class="card-body">
                        <div class="d-flex justify-content-between"><h2>All Student</h2> <a href="/create" class="btn btn-success">create New Student</a></div>
                        <hr>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Name</th>
                                    <th>E-mail</th>
                                    <th>Call</th>
                                    <th>Photo</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% Student.forEach((element, index) => { %>
                                    <tr>
                                        <td><%= index+1 %> </td>
                                        <td><%= element.name %> </td>
                                        <td><%= element.email %></td>
                                        <td><%= element.call %></td>
                                        <td><img src="/assets/upload/<%= element.photo %> " alt="" style="width:40px; height:40px; object-fit:cover; border-radious:50%;"></td>
                                        <td>
                                            <a href="/show/<%= element._id %>" class="btn btn-success"><i class="fas fa-eye"></i></a>
                                            <a href="/edit/<%= element._id %>" class="btn btn-warning"><i class="fas fa-edit"></i></a>
                                            <a href="/delete/<%= element._id %>" class="btn btn-danger"><i class="fas fa-trash"></i></a>
                                        </td>
                                    </tr>
                                <% }) %>
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>

